<div style="margin-top:50px;"></div>

<div class="department-header">
<table class="table tb-file-viewer-header">
  <thead class="thead-default">
    <tr>
      <th style="width:17%; padding-left:15px;" class="thead-default-tr-th">Grant Number</th>
      <th style="width:4%;"  class="thead-default-tr-th">PI</th>
      <th style="width:10%;" class="thead-default-tr-th">Date Submitted</th>
      <th style="width:10%;" class="thead-default-tr-th">Date Published</th>
      <th style="width:8%;"  class="thead-default-tr-th">PI Email</th>
      <th style="width:50%;" >{{status-filter}}</th>
    </tr>
  </thead>
 </table>
</div>



<div class="document-list">

  <div class="document" data-item=".archived">

      <div class="less-info">
        <i class="fa fa-file-text" aria-hidden="true"></i>
        <span class="grant-number"><input size="10" type="text" name="grant-number" value="##########" readonly="readonly"></span>
        <span class="pi"><input size="12" type="text" name="pi" value="Leo Leogibjnsdblnablj" readonly="readonly"></span>
        <span class="date-submitted"><input size="7" type="text" name="date-submitted" value="99/99/99" readonly="readonly"></span>
        <span class="date-published"><input size="7" type="text" name="date-published" value="1/1/1" readonly="readonly"></span>
        <span class="pi-email"><input size="17" type="text" name="pi-email" value="pi-email@email.email" readonly="readonly"></span>

      </div>


      <ul class="action-buttons">
        <li class='more-info-button' {{action "moreInfo" this}}>More Info<span class="caret"></span></li>
        <li class='view-document-button' {{action "viewDocument" this}}>View Document<span class="caret"></span></li>
        <li>Download</li>
      </ul>

      <div class='more-info'>
        <table>
              <thead>
                <tr>
                  <th scope="row">
                    File Name
